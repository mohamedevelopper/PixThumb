{
  "name": "pixthumb-downloader",
  "version": "1.0.0",
  "description": "Free YouTube thumbnail downloader - Download HD thumbnails instantly",
  "keywords": [
    "youtube",
    "thumbnail",
    "downloader",
    "free",
    "hd",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run sitemap && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "export": "next build && next export",
    "deploy": "npm run export && touch out/.nojekyll && git add out/ && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix out origin gh-pages",
    "sitemap": "node scripts/generate-sitemap.js",
    "postbuild": "npm run sitemap"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "gray-matter": "^4.0.3",
    "jszip": "^3.10.1",
    "next": "14.2.4",
    "next-themes": "^0.4.6",
    "react": "^18",
    "react-dom": "^18",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "globby": "^14.0.0",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}